{
    "manifest_version": 2,

    "name": "Beat Saber Dark Mode",
    "description": "Applies a dark theme to beatsaver.com and scoresaber.com",
    "version": "1.0",


    "content_scripts": [
        {
            "matches": [
                "https://beatsaver.com/*"
            ],
            "css": [
                "beatsaver.css"
            ]
        }
    ]
}